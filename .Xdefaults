URxvt.geometry: 90x30+256+64
URxvt.visualBell: False
urxvt*inheritPixmap: true
URxvt.tintColor: white
URxvt.shading: 80
URxvt.perl-ext-common: default,tabbed
urxvt*fading: 80
urxvt*title:Rxvt-Unicode
urxvt*saveLines: 65535
Urxvt*underlineurls: true

! scroll
URxvt*scrollstyle:           plain
URxvt*mouseWheelScrollPage:  1
URxvt*cursorBlink:           1
URxvt*scrollTtyOutput:       false
URxvt*scrollWithBuffer:      true
URxvt*scrollTtyKeypress:     true
!urxvt*scrollBar: false

!urxvt*borderLess: true

! color
!urxvt*colorBD: white
!urxvt*colorIT: green
URxvt.background: black
URxvt.foreground: green
!URxvt.cursorColor: red3
!URxvt.color0: #000000
!URxvt.color1: #800000
!URxvt.color2: #008000
!URxvt.color3: #d0d090
!URxvt.color4: #000080
!URxvt.color5: #800080
!URxvt.color6: #d0d0d0
!URxvt.color7: #b0b0b0
!URxvt.color8: #f08060
!URxvt.color9: #f08060
!URxvt.color10: #60f080
!URxvt.color11: #e0c060
!URxvt.color12: #80c0e0
!URxvt.color13: #f0c0f0
!URxvt.color14: #c0d8f8
!URxvt.color15: #e0e0e0
!URxvt.preeditType: Root

! font
URxvt*font:xft:Inconsolata:size=16,xft:Noto Sans CJK TC:style=Light:size=16,xft:DejaVu Sans Mono,xft:Symbola
URxvt*internalBorder:        1
!URxvt.font: xft:DejaVu Sans Mono-14,xft:AR PL ShanHeiSun Uni-14
URxvt.boldFont: xft:DejaVu Sans Mono-14:Bold,xft:AR PL ShanHeiSun Uni-14:Bold

!輸入法
inputMethod: fcitx
!輸入法樣式可選:Root(置底) OverTheSpot(跟隨)
URxvt.preeditType: Root

!增加tab的功能
URxvt.perl-ext-common: default,tabbed

!shift+down：新增tab
!shif+left：左移
!shift+right：右移
