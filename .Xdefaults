URxvt.geometry: 90x30+256+64
URxvt.visualBell: False
urxvt*inheritPixmap: true
URxvt.tintColor: white
URxvt.shading: 80
URxvt.perl-ext-common: default,tabbed
urxvt*fading: 80
urxvt*title:Rxvt-Unicode
urxvt*saveLines: 65535
Urxvt*underlineurls: true

! scroll
URxvt*scrollstyle:           plain
URxvt*mouseWheelScrollPage:  1
URxvt*cursorBlink:           1
URxvt*scrollTtyOutput:       false
URxvt*scrollWithBuffer:      true
URxvt*scrollTtyKeypress:     true
!urxvt*scrollBar: false

!urxvt*borderLess: true

! color
!urxvt*colorBD: white
!urxvt*colorIT: green
URxvt.background: black
URxvt.foreground: green
! black
URxvt.color0  : #000000
URxvt.color8  : #555555
! red
URxvt.color1  : #AA0000
URxvt.color9  : #FF5555
! green
URxvt.color2  : #00AA00
URxvt.color10 : #55FF55
! yellow
URxvt.color3  : #AA5500
URxvt.color11 : #FFFF55
! blue
URxvt.color4  : #0000AA
URxvt.color12 : #5555FF
! magenta
URxvt.color5  : #AA00AA
URxvt.color13 : #FF55FF
! cyan
URxvt.color6  : #00AAAA
URxvt.color14 : #55FFFF
! white
URxvt.color7  : #AAAAAA
URxvt.color15 : #FFFFFF

! font
URxvt*font:xft:Inconsolata:size=16,xft:Noto Sans CJK TC:style=Light:size=16,xft:DejaVu Sans Mono,xft:Symbola
URxvt*internalBorder:        1
!URxvt.font: xft:DejaVu Sans Mono-14,xft:AR PL ShanHeiSun Uni-14
URxvt.boldFont: xft:DejaVu Sans Mono-14:Bold,xft:AR PL ShanHeiSun Uni-14:Bold

!輸入法
inputMethod: fcitx
!輸入法樣式可選:Root(置底) OverTheSpot(跟隨)
URxvt.preeditType: Root

!增加tab的功能
URxvt.perl-ext-common: default,tabbed

!shift+down：新增tab
!shif+left：左移
!shift+right：右移
